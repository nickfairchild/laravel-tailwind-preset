{
    "name": "nickfairchild/laravel-tailwind-preset",
    "description": "Laravel frontend preset for Tailwind CSS",
    "keywords": [
        "laravel",
        "frontend",
        "preset",
        "tailwind"
    ],
    "homepage": "https://github.com/nickfairchild/laravel-tailwind-preset",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nick Fairchild",
            "email": "nick.fairchild@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "illuminate/support": "^7.0",
        "laravel/ui": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Nickfairchild\\TailwindPreset\\": "src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nickfairchild\\TailwindPreset\\TailwindPresetServiceProvider"
            ]
        }
    }
}
